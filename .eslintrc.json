{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "no-param-reassign" : 0,
        "no-unused-vars": ["warn"],
        "max-len": ["warn", { 
            "code": 120,
            "ignoreComments": true 
            }
        ],
        "linebreak-style": ["error", "unix"]
    },
    "globals": {
        "window": true,
        "document": true
    }
}
