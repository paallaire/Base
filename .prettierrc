{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 120,
  "htmlWhitespaceSensitivity": "ignore",
  "plugins": [
    "prettier-plugin-tailwindcss"
  ],
  "tempplugins": [
    "prettier-plugin-twig-melody",
    "prettier-plugin-tailwindcss"
  ],
  "pluginSearchDirs": false
}
