{
  "name": "base",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development TASK=dev webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development TASK=watch webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production TASK=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "tailwind": "cross-env NODE_ENV=tailwind TASK=tailwind webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "stylelint": "stylelint assets/styles/**/*.scss --syntax scss --fix",
    "eslint": "eslint --fix assets/scripts/**/*.js tailwind.config.js",
    "lint": "eslint assets/scripts/**/*.js tailwind.config.js",
    "lint:fix": "eslint --fix",
    "tailwind-scss-var": "node assets/tailwindcss/generate-scss-variables.js",
    "format:prettier": "node_modules/.bin/prettier --config .prettierrc \"assets/**/*.{ts,css,less,js}\" --write",
    "fractal": "fractal start --sync",
    "fractal:build": "fractal build"
  },
  "fractal": {
    "main": "fractal.config.js"
  },
  "devDependencies": {
    "@frctl/fractal": "^1.2.1",
    "@frctl/twig": "^1.1.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "babel-eslint": "^10.0.3",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-svg-sprite": "^1.5.0",
    "laravel-mix": "^5.0.1",
    "laravel-mix-purgecss": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.2.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-sass-guidelines": "^7.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "dom4": "^2.1.5",
    "hamburgers": "^1.1.3",
    "include-media": "^1.4.9",
    "include-media-export": "^1.0.3",
    "lodash": "^4.17.15",
    "picturefill": "^3.0.3",
    "postcss-pxtorem": "^4.0.1",
    "regenerator-runtime": "^0.13.3",
    "scroll-lock": "^2.1.2",
    "svgxuse": "^1.2.6",
    "tailwindcss": "^1.2.0",
    "vanilla-lazyload": "^12.4.0",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "webfontloader": "^1.6.28",
    "whatwg-fetch": "^3.0.0"
  }
}
