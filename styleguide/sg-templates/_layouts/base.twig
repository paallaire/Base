

<!DOCTYPE html>
<html class="sg-html" lang="en">

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <title>{{ title }} | Styleguide</title>
    <link href="/dist/styles/styleguide.css" rel="stylesheet">
    <link href="/dist/styles/main.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,700" rel="stylesheet">

    {% block head %}{% endblock %}

</head>
<body class="sg-body">
    <div id="app">
        <div class="l-wrapper">
        {% block page %}{% endblock %}
        </div>
        {% include "../_partials/menu.twig" %}
        <show-grid :columns="12" :container-class="'l-wrapper'" :row-class="'lost-row'" :column-class="'lost-column-1'"></show-grid>
    </div>
    <script src="/dist/scripts/styleguide.js"></script>
    {% block afterMainScript %}{% endblock %}
</body>
</html>
